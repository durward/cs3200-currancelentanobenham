<!DOCTYPE html>
<html>
<head>
</head>

<body>
    <h1>List of Senators</h1>
    <table>
      <tr>
        <th>ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>State</th>
        <th>Party</th>
        <th>Website</th>
      </tr>
      <% senators.forEach(function(r) { %>
      <tr>
        <td> <%= r.senid %> </td>
        <td> <%= r.fname %> </td>
        <td> <%= r.lname %> </td>
        <td> <%= r.state %> </td>
        <td> <%= r.party %> </td>
        <td> <%= r.website %> </td>
      </tr>
      <% }); %>
      <form action="/insert" method="post">
        <tr> <input type="text" name="id"> </tr>
        <tr> <input type="text" name="fname"> </tr>
        <tr> <input type="text" name="lname"> </tr>
        <tr> <input type="text" name="state"> </tr>
        <tr> <input type="text" name="party"> </tr>
        <tr> <input type="text" name="website"> </tr>
        <tr> <input type="submit" value="submit"> </tr>
      </form>
    </table>
</body>
</html>
