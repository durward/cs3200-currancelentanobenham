<!DOCTYPE html>
<html>
<head>
</head>

<body>
    <h1>List of Senators</h1>
    <table>
      <tr>
        <th>ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>State</th>
        <th>Party</th>
        <th>Website</th>
      </tr>
      <% senators.forEach(function(r) { %>
      <tr>
        <td> <%= r.senid %> </td>
        <td> <%= r.fname %> </td>
        <td> <%= r.lname %> </td>
        <td> <%= r.state %> </td>
        <td> <%= r.party %> </td>
        <td> <%= r.website %> </td>
      </tr>
      <% }); %>
    </table>
</body>
</html>
