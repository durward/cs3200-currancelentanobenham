<!DOCTYPE html>
<html>
<head>
</head>

<body>
    <h1>List of Senators</h1>
    <table>
      <tr>
        <th>ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>State</th>
        <th>Party</th>
        <th>Website</th>
      </tr>
      <% senators.forEach(function(r) { %>
      <tr>
        <td> <%= r.senid %> </td>
        <td> <%= r.fname %> </td>
        <td> <%= r.lname %> </td>
        <td> <%= r.state %> </td>
        <td> <%= r.party %> </td>
        <td> <%= r.website %> </td>
      </tr>
      <% }); %>
      <tr>
        <form action="/insert" method="post">
          <td> <input type="text" id="insert-id" name="id" placeholder="ID"> </td>
          <td> <input type="text" id="insert-fname" name="fname" placeholder="First Name"> </td>
          <td> <input type="text" id="insert-lname" name="lname" placeholder="Last Name"> </td>
          <td> <input type="text" id="insert-state" name="state" placeholder="State"> </td>
          <td> <input type="text" id="insert-party" name="party" placeholder="Party"> </td>
          <td> <input type="text" id="insert-website" name="website" placeholder="Website"> </td>
          <td> <input type="submit" value="Insert"> </td>
        </form>
      <tr>
    </table>
</body>
</html>
